<%- include("../includes/head")%>
</head>

<body>
    <%- include("../includes/navigation")%>
    <main>
        <% if(orders.length > 0) {%>
            <ul>
                <% orders.forEach(order => { %>
                    <li>
                        <h1># <%= order.id %></h1>
                        <ul>
                            
                                <% order.products.forEach(product => { %>
                                <li><%= product.title %> (<%= product.orderItems.quantity %>)</li>
                                <%}) %>
                             
                        </ul>
                    </li>
                <% }) %> 
            </ul>
        <%} else {%>
            <h1>Nothing there!</h1>
        <%}%>
    </main>
<%- include("../includes/end") %>